{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "|uniquestring|/full-width-section",
	"version": "0.1.0",
	"title": "Full width section",
	"category": "widgets",
	"icon": "align-wide",
	"description": "Add a Full width section",
	"supports": {
		"html": false,
		"align": [
			"left",
			"right",
			"full"
		],
		"anchor": true,
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": ""
		},
		"message": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": "Hello, World!"
		},
		"mediaBGId": {
			"type": "string",
			"source": "attribute",
			"selector": ".|uniquestring|-full-width-inner",
			"attribute": "data-image-id",
			"default": null
		},
		"mediaBGUrl": {
			"type": "string",
			"source": "attribute",
			"selector": ".|uniquestring|-full-width-inner",
			"attribute": "data-image-url",
			"default": null
		}
	},
	"textdomain": "|uniquestring|-domain",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}