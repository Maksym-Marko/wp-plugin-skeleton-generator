{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "|uniquestring|/counter-section",
	"version": "0.1.0",
	"title": "Counter Section",
	"category": "design",
	"icon": "smiley",
	"description": "Add a counter section",
	"supports": {
		"html": false,
		"align": [
			"left",
			"right",
			"full"
		],
		"color": {
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": ""
		},
		"mediaId": {
			"type": "string",
			"source": "attribute",
			"selector": ".mxImageSection",
			"attribute": "data-image-id",
			"default": null
		},
		"mediaUrl": {
			"type": "string",
			"source": "attribute",
			"selector": ".mxImageSection",
			"attribute": "data-image-url",
			"default": null
		},
		"message": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": "Hello, World!"
		}
	},
	"textdomain": "mx-|uniquestring|",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}