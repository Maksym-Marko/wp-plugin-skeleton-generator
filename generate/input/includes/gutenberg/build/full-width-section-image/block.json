{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "|uniquestring|/full-width-section-image",
  "version": "0.1.0",
  "title": "Full width section (Side Image)",
  "category": "widgets",
  "icon": "align-wide",
  "description": "Add a Full width section",
  "supports": {
    "html": false,
    "align": [
      "left",
      "right",
      "full"
    ],
    "anchor": true,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    }
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "mediaBGId": {
      "type": "string",
      "source": "attribute",
      "selector": "div",
      "attribute": "data-image-id",
      "default": null
    },
    "mediaBGUrl": {
      "type": "string",
      "source": "attribute",
      "selector": "div",
      "attribute": "data-image-url",
      "default": null
    },
    "sideImageUrl": {
      "type": "string",
      "source": "attribute",
      "selector": ".mx-side-img",
      "attribute": "src",
      "default": null
    },
    "sideImageId": {
      "type": "string",
      "source": "attribute",
      "selector": ".mx-side-img",
      "attribute": "data-side-image-id",
      "default": null
    },
    "imageToRight": {
      "type": "string",
      "source": "attribute",
      "selector": "div",
      "attribute": "data-image-to-right",
      "default": "0"
    }
  },
  "textdomain": "|uniquestring|-domain",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}